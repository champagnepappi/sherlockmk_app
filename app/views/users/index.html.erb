<table>
    <tr>
        <th><%= link_to "Name", users_path(sort:"name" ) %></th>
        <th>Email</th>
        <th>Joined</th>
        <th colspan="5"></th>
    </tr>
<% @users.each do |user| %>
    <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
    </tr>
<%end%>
</table>
